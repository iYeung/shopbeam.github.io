};

	  this.getUnitDomReferences = function (){        
        playerObject.adUnitContainerDomReference = document.getElementById("adUnitContainer" + playerObject.unitUniqueId);
					playerObject.creativeUnitDomReference = document.getElementById("banner" + playerObject.unitUniqueId);
						playerObject.expansionPromptContainerDomReference = document.getElementById("expansionPromptContainer" + playerObject.unitUniqueId);
		playerObject.playBtnExpandUnitContainerDomReference = document.getElementById("playBtnExpandUnitContainer" + playerObject.unitUniqueId);    
		      
		playerObject.playerExInteractivityDomReference = document.getElementById("interactivityExpandedPanel" + playerObject.unitUniqueId);
		        playerObject.playerExCloseInteractivityBtnDomReference = document.getElementById("closeExpandedInteractivityBanner" + playerObject.unitUniqueId);
		        playerObject.playerExInteractivityLoaderDomReference = document.getElementById("intrExpandedLoader" + playerObject.unitUniqueId);
        playerObject.playerExInteractivityFrameDomReference = document.getElementById("intrExpandedDetailPanel" + playerObject.unitUniqueId);
        playerObject.adUnitAnchorDomReference = document.getElementById("adUnitAnchor" + playerObject.unitUniqueId);

	  };

	 this.attachEvents = function(){

		try{
		
					attachEventListener(playerObject.adUnitContainerDomReference, "mouseover", function(e){
				playerObject.DwellRecorded = false;
				
				if(null == playerObject.DwellElapsedTimer){
					playerObject.DwellElapsedTimer = setInterval(function(){ playerObject.DwellElapsedTime = playerObject.DwellElapsedTime+1;}, 1000);
				}
				
				if(null == playerObject.DwellTimer){
					playerObject.DwellTimer = setTimeout(function(){playerObject.recordDwellEvent(49, playerObject.DwellElapsedTime); },60000);
				}
				
				if(!playerObject.UIRRecorded){
					playerObject.UIRTimer = setTimeout(function(){playerObject.recordUIREvent(48);},500);
				}
				playerObject.cancelEvent(e);
			}, false);
		
		

					playerObject.slideOutLargeAdUnit('47863','2521715ca13bed', '1');
			setTimeout(function(){playerObject.slideInLargeAdUnit(true);},1000000 * 1000);
				

		
				
                if(playerObject.adUnitAnchorDomReference)
    		attachEventListener(playerObject.adUnitAnchorDomReference, "click", function(e){
				playerObject.userDidClickOnAnchor = true;
								return true;
			}, false);
        
				
     
			if(typeof(playerObject.playerExCloseInteractivityBtnDomReference) != "undefined" && playerObject.playerExCloseInteractivityBtnDomReference != null){
            attachEventListener(playerObject.playerExCloseInteractivityBtnDomReference, "click", function(e){playerObject.slideInLargeAdUnit();}, false);
			}
				
		
				
				attachEventListener(playerObject.playerExInteractivityDomReference, "mouseover", function(e){
		playerObject.adUnitMouseout=false;
				playerObject.DwellRecorded = false;
		
		if(null == playerObject.DwellElapsedTimer){
			playerObject.DwellElapsedTimer = setInterval(function(){ playerObject.DwellElapsedTime = playerObject.DwellElapsedTime+1; }, 1000);
		}
		
		if(null == playerObject.DwellTimer){
			playerObject.DwellTimer = setTimeout(function(){playerObject.recordDwellEvent(49, playerObject.DwellElapsedTime); },60000);
		}
		
		if(!playerObject.UIRRecorded){
			playerObject.UIRTimer = setTimeout(function(){playerObject.recordUIREvent(48);},500);
		}
				playerObject.cancelEvent(e);} , false);
		
		
        
        attachEventListener(window, "message", receiveHtml5Message, false);
		
		
			
				if("onorientationchange" in window){
			attachEventListener(window, "orientationchange", playerObject.propagateOrientationChange, false);
		}else if("onresize" in window){
			attachEventListener(window, "resize", playerObject.propagateOrientationChange, false);
		}
		
		}catch(e){
						setTimeout(function(){playerObject.getUnitDomReferences(); playerObject.attachEvents();}, 500);
		}

	 };	


function getViewportDim() {
    var e = window, a = 'inner';
    if (!('innerWidth' in window )) {
        a = 'client';
        e = document.documentElement || document.body;
    }
    return { width : e[ a+'Width' ] , height : e[ a+'Height' ] };
}

	 	    this.propagateOrientationChange = function(){
	    		    		var playerPos = findPos(playerObject.playBtnExpandUnitContainerDomReference);
	    		var maskWinXY = getCenterAlignXY(640,480);
	    		var unitLeftPosition = maskWinXY[0] - playerPos[0];
	    		var unitTopPosition = maskWinXY[1] + playerPos[3] - playerPos[1];
				playerObject.playerExInteractivityDomReference.style.right = "";
				playerObject.playerExInteractivityDomReference.style.left = "";
				playerObject.playerExInteractivityDomReference.style.left = (unitLeftPosition) + "px";
				playerObject.playerExInteractivityDomReference.style.top = (unitTopPosition) + "px";
	    	
	    	        };
     
	 
     
    function receiveHtml5Message(e)
    {
                var s = e.data.split(':', 10);
        if(s && s.length >= 2 && s[0] == playerObjectId)
        {
            if(s[1] == 'closeAdUnit') {
                playerObject.closeAdUnit();
			}else if(s[1] == 'slideInLargeAdUnit'){
				if(s[2] == 'preventSlideEvent')
					playerObject.slideInLargeAdUnit(true);
				else
					playerObject.slideInLargeAdUnit();
			}else if(s[1] == 'slideOutLargeAdUnit'){
				if(s[2] == 'preventSlideEvent')
					playerObject.slideOutLargeAdUnit(null, null, true);
				else
					playerObject.slideOutLargeAdUnit();
			}
        }
    }


    /* Function to create a cookie */
    function createCookieObj(name,value,days,domainPath) {
        if (days) {
                var date = new Date();
                date.setTime(date.getTime()+(days*24*60*60*1000));
                var expires = "; expires="+date.toGMTString();
        } else {
            var expires = "";
        }
        document.cookie = name+"="+value+expires+"; path=/; domain="+domainPath;
    };
    
    /* Function to delete a cookie */
    
    function eraseCookieObj(name) {
            createCookieObj(name,"",-1,".jivox.com");
    };
    
    /* function to read cookie val */
    function readCookieObj(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
    
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        
        return null;
    };

	
	this.recordUIREvent = function(eventId, elapsedTime){
		if(playerObject.UIRRecorded){
			return;
		}
				recordEvent(eventId, elapsedTime);
				playerObject.UIRRecorded = true;
	};
	
	this.recordDwellEvent = function(eventId, elapsedTime){
		if(playerObject.DwellRecorded){
			return;
		}
		clearTimeout(playerObject.DwellTimer);
				if(elapsedTime > 0){
			recordEvent(eventId, elapsedTime);
		}
				clearInterval(playerObject.DwellElapsedTimer);
		playerObject.DwellElapsedTimer = null;
        playerObject.DwellElapsedTime = 0;
		playerObject.DwellRecorded = true;
		playerObject.DwellTimer = null;
	};

	

	function recordEvent(eventId, elapsedTime){

		
		var rand = Math.random();	
		
		var eventRecordURL = 'http://evs.jivox.com/jivox/serverAPIs/externalReporting/tracker.php?campaignId=47863&siteId=2521715ca13bed&clickThrough='+eventId+'&adUnitType=9&jvxSessionId=1380317264.9409&random=' + rand;

		eventRecordURL += '&es_pId=' + '';
		
		if(elapsedTime != undefined && elapsedTime != '' && elapsedTime != 0){
		 eventRecordURL += '&elapsedTime=' + elapsedTime;
		}

		var imgEl = document.createElement('img');
		imgEl.src = eventRecordURL;

			}

	 this.retractOpenPanel = function(e){
		     };


	function getCenterAlignXY(objWidth,objHeight){ 
		 var winPos = getBrowserWindowProps();
		 var objX = ((winPos[0]- 28) - objWidth) / 2 ;
		 var objY = (winPos[1] - objHeight) / 2 ;
		 objX = (objX < 0) ? 0 : objX;
		 objY = (objY < 0) ? 0 : objY;
		 return [objX,objY];
	}

	function setOpacity(myElement, opacityValue) {
		if(typeof myElement.style.opacity !== 'undefined'){
			myElement.style.opacity = opacityValue/100; // Gecko/Opera
		} else {
			myElement.style.filter = "alpha(opacity="
				 + opacityValue + ")"; // IE			
		}
	}

	this.injectMask = function(){
		if(document.getElementById("mask" + playerObject.unitUniqueId) != null || document.getElementById("mask" + playerObject.unitUniqueId) != undefined){
			playerObject.maskDomReference.style.display = 'block';
			return;
		}
		var maskDiv = document.createElement('div');
		maskDiv.id = "mask" + playerObject.unitUniqueId;
		var styleObj = {'position':'fixed','top': '0px','left': '0px','width': '100%','background': '#FFFFFF repeat scroll 0 0','display': 'block'};

		for(var i in styleObj){
			maskDiv.style[i] = styleObj[i];
		}

		setOpacity(maskDiv,60);
		var maskHeight = (document.body.scrollHeight < window.innerHeight) ? window.innerHeight : document.body.scrollHeight;		
		maskDiv.style.height = maskHeight + 'px';
		maskDiv.style.zIndex = 50;
		document.body.appendChild(maskDiv);
		playerObject.maskDomReference = document.getElementById("mask" + playerObject.unitUniqueId);
	}
	 this.slideOutLargeAdUnit = function(campaignIdx, siteIdx, autoExpand){	

			if(playerObject.interactivityState == 'open'){
				return;
			}

			
				

						
						playerObject.injectMask();
								
			clearInterval(playerObject.showInter);
			clearInterval(playerObject.hideInter);
			clearInterval(playerObject.showPlayInterval);
			clearTimeout(playerObject.hidePlayTimeout);
			clearInterval(playerObject.creativeUnitHoverTimer);
			clearInterval(playerObject.creativeUnitHoverTimerOnce);
			playerObject.creativeUnitHoverTimeForExpand = 0;
						
			    						
            if(playerObject.playerExInteractivityLoaderDomReference != null && playerObject.playerExInteractivityLoaderDomReference.innerHTML == ''){
                playerObject.playerExInteractivityLoaderDomReference.innerHTML = '<img style="width:32px; height:32px; border:0px; margin:0px; padding:0px; position:absolute; top:'+ (playerObject.largeAdUnitHeight - 32) / 2 +'px; left: '+ (playerObject.largeAdUnitWidth - 32) / 2 +'px;" src="http://'+staticResourceURL+'images/loader-animation-2.gif" alt="Loading..." />';
            }
			        
                        
            campaignId = (typeof(campaignIdx) != "undefined" && !isNaN(campaignIdx)) ? campaignIdx : campaignId;
            siteId = (typeof(siteIdx) != "undefined") ? siteIdx : siteId;
            setLargeAdUnitSlideOutOrientation();
            
                        
			            playerObject.playerExCloseInteractivityBtnDomReference.style.display = "block";
			                        
            
            var interactivityUrl = null;	
			
			var adUnitDimParams = "";

						adUnitDimParams = "&adUnitWidth=" + playerObject.largeAdUnitWidth + "&adUnitHeight=" + playerObject.largeAdUnitHeight;
			
                        interactivityUrl= "http://as.jivox.com/player/expandedUnit.php?expandAsInterstitial=1&t=1380317264318&r=0.4445457870606333&creativeUnitType=8&creativeUnitWidth=640&creativeUnitHeight=480&siteId=2521715ca13bed&campaignId=47863&clickTagURL=http%3A%2F%2Fadclick.g.doubleclick.net%2Faclk%3Fsa%3DL%26ai%3DB4Z7GUPhFUpLQE-vP0AG93ICIDaqowo0EAAAAEAEgwqCnDjgAWOqL5uphYMnGqYvApNgPsgEWd3d3LmNvc21vZW5lc3Bhbm9sLmNvbboBCzAwZ2ZwX2ltYWdlyAEJ2gEmaHR0cDovL3d3dy5jb3Ntb2VuZXNwYW5vbC5jb20vZmFzaGlvbi-pAo2ngfDMYXQ-wAIC4AIA6gI-LzcxODcvdGVsZXZpc2EuZWRpdG9yaWFsLmNvc21vZW5lc3Bhbm9sL2Nvc21vZW5lc3Bhbm9sLmZhc2hpb274AoHSHpAD4AOYA6QDqAMB0ASQTuAEAaAGIA%26num%3D0%26sig%3DAOD64_1auFdJJrgb5qh-EFPUxfZCfZCLWw%26client%3Dca-pub-7798366957477628%26adurl%3Dhttp%3A%2F%2Fwww.MiPantene.com&objectName=jvxAdPlayer_5245f850874f99_49863897&creativeUnitType=14&lDim=640x480&expandUnitType=&adUnitType=9&expandedUnit=1&viewLayout="+adUnitDimParams+ "&jvxSessionId=1380317264.9409";
                        if(typeof(autoExpand) == 'undefined' || !autoExpand)
                interactivityUrl += "&userInitAction=1";

            // if parent (us) are using html5, tell expandedUnit to use it too.
            if(this.useHTML5)
                interactivityUrl += "&usingHTML5=1";
            if(this.useHTML5VideoSync)
                interactivityUrl += "&usingHTML5VideoSync=1";

			
			                
            return slideOutLargeUnit(playerObject.playerExInteractivityDomReference.id, interactivityUrl, "expandedUnit", autoExpand);
        }

	  function setLargeAdUnitSlideOutOrientation(){

        var playerPos = findPos(playerObject.playBtnExpandUnitContainerDomReference);
        var winPos = getBrowserWindowProps();
        var availableScreenWidth = winPos[0] - 28; //removed scrollbar from overall space
		var availableScreenHeight = winPos[1];
        var playerLeftPos = playerPos[0];
		var leftAvailableSpace = playerLeftPos;
		var rightAvailableSpace = availableScreenWidth - (playerLeftPos + 1);
		var adUnitLeftOffset = 0;
		
				var maskWinXY = getCenterAlignXY(640,480);
		playerObject.playerExInteractivityDomReference.style.right = "";
		playerObject.playerExInteractivityDomReference.style.left = "";
		playerObject.playerExInteractivityDomReference.style.left = (maskWinXY[0] - playerLeftPos) + "px";
		return;
				
				
		        
         
		        if((playerLeftPos + 0) > playerObject.largeAdUnitWidth){
			var newRight = 0;		
			            playerObject.playerExInteractivityDomReference.style.right = "";
            playerObject.playerExInteractivityDomReference.style.left = "";
            playerObject.playerExInteractivityDomReference.style.right = newRight + adUnitLeftOffset + "px";   
			        }else{
			var newLeft = 0;
						if((playerLeftPos + 0 + playerObject.largeAdUnitWidth) > availableScreenWidth){					
				if(playerObject.largeAdUnitWidth - leftAvailableSpace < leftAvailableSpace){
					 var newRight = -(playerObject.largeAdUnitWidth - playerLeftPos);
					 					 playerObject.playerExInteractivityDomReference.style.right = "";
					 playerObject.playerExInteractivityDomReference.style.left = "";
					 playerObject.playerExInteractivityDomReference.style.right = newRight + adUnitLeftOffset + "px"; 
									}else{
					newLeft = -playerLeftPos+0;
										playerObject.playerExInteractivityDomReference.style.right = "";
					playerObject.playerExInteractivityDomReference.style.left = "";
					playerObject.playerExInteractivityDomReference.style.left = newLeft + adUnitLeftOffset + "px";  
									}
			} else {
					playerObject.playerExInteractivityDomReference.style.right = "";
					playerObject.playerExInteractivityDomReference.style.left = "";
					playerObject.playerExInteractivityDomReference.style.left = newLeft + adUnitLeftOffset + "px";
								}
                  
        }
						 
    };

	
	
	  function slideOutLargeUnit(elementId, interactivityURL, panelId, autoExpand)
    {
        
       var element = document.getElementById(elementId);
       var playerPos = findPos(playerObject.playBtnExpandUnitContainerDomReference);
       var winPos = getBrowserWindowProps();
	          var unitExpandWidth = playerObject.largeAdUnitWidth;
       var unitExpandHeight = playerObject.largeAdUnitHeight;
       var unitExpandSpeed = 250;
       //position the player 10px from the top
       var unitTopPosition = 0;

								if((winPos[1] - (playerPos[1] - playerPos[3])) > playerObject.largeAdUnitHeight){
				if(playerPos[1] > playerPos[3]){
					unitTopPosition = 0;
				}else{
					unitTopPosition = playerPos[3] - playerPos[1];
				}
			}else {
				unitTopPosition = (playerObject.largeAdUnitHeight - (winPos[1] - (playerPos[1] - playerPos[3])));
				if(unitTopPosition < (playerPos[1] - playerPos[3])){
					unitTopPosition = -unitTopPosition;
				}else {
					unitTopPosition = unitTopPosition - (unitTopPosition - (playerPos[1] - playerPos[3]));
					unitTopPosition = -unitTopPosition;
				}
			}
					
	   
       var unitLeftPosition = element.style.left;

	   
	 
       
	   
	   	   var maskWinXY = getCenterAlignXY(640,480);
	   unitTopPosition = maskWinXY[1] + playerPos[3] - playerPos[1];
	   	     
       var objIframe = element.getElementsByTagName("iframe");
       iframeURL = objIframe[0].src; //.toLowerCase();

       //var urlRegExp = new RegExp(regExpEscape(interactivityURL).toLowerCase());

       //if(!urlRegExp.test(iframeURL)){
       if(iframeURL != interactivityURL){
           objIframe[0].style.display = "block";
                      objIframe[0].style.visibility = "hidden";
                      var intrLoader = objIframe[0].parentNode.firstChild;
           intrLoader.style.display = "block";
           attachEventListener(objIframe[0], "load", function(){contentLoad(objIframe[0], panelId)}, false);
           objIframe[0].src = interactivityURL;
        }

					element.style.visibility = 'visible';
			element.style.display = 'block';
		//elementID, newLeft, newTop, newWidth, newHeight, time, callback
		animate(elementId, unitLeftPosition, unitTopPosition, unitExpandWidth, unitExpandHeight, unitExpandSpeed, completedSlideOutLargeUnit);
		       
       
		
		playerObject.interactivityState = "open";
				playerObject.playerExCloseInteractivityBtnDomReference.style.display = 'block';
		
		if(!autoExpand){
			recordEvent(37, 0); // record user initiated expand unit open event.		
					}
		playerObject.expandedUnitPanelTimer = setInterval(function(){ playerObject.expandedUnitTimeElapsed = playerObject.expandedUnitTimeElapsed+1; }, 1000);
		
       //return flag to the player indicating the expanding unit is on and pause the ad
       return false;

    };

        
