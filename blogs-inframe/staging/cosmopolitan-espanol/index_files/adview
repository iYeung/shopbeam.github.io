	function onSlideInComplete(){
		try{
			document.body.style.position = "relative";
			document.body.offsetHeight;
			document.body.style.position = "";
		}catch(e){
		}
	}
	
	function onSlideInOver() {
		playerObject.playerExInteractivityDomReference.style.visibility = 'hidden';
		playerObject.playerExInteractivityDomReference.style.display = 'none';
        playerObject.interactivityState = "close"; //Fix for ADPTL-4784
						playerObject.playerExCloseInteractivityBtnDomReference.style.visibility = 'visible';
		playerObject.playerExCloseInteractivityBtnDomReference.style.display = 'block';
								onSlideInComplete();
		
	}

	
	function slideIn(elementId, interactivityURL, panelId)
    {
       var element = document.getElementById(elementId);      
	   var objIframe = element.getElementsByTagName("iframe");
	   	   var callbackFn = onSlideInComplete;
	   	   
       iframeURL = objIframe[0].src.toLowerCase();
       var urlRegExp = new RegExp(regExpEscape(interactivityURL).toLowerCase());
           
       if(!urlRegExp.test(iframeURL)){		
           objIframe[0].src = interactivityURL;
           var intrLoader = objIframe[0].parentNode.firstChild;
           intrLoader.style.display = "block";
           objIframe[0].style.display = "none";
       } 

              animate(elementId, 0, 0, 0, 0, 250, callbackFn);
	   //animate(elementId, 0, 0, 1, 1, 250, onSlideInOver);
	          destroyInteractivityPanel = true;  	 
		
		
				if(playerObject.maskDomReference != null && playerObject.maskDomReference != undefined)
			playerObject.maskDomReference.style.display = 'none';
		    };
	
	var countDownAnimTimer = function(dimension, countDownSeconds, parentObj, useDefault) {
		//"private" variables:
		var counter, defaultCounter = 3;
		if(useDefault) {
			counter = defaultCounter;
		} else {
			counter = countDownSeconds;
		}
		var width = height = dimension,
			centerx = centery = dimension/2,
			circleRadius = dimension/3,
			circleTopColor = '#bababa',
			circleBottomColor = '#7c7c7c',
			circleOpacity = 0.75,
			circleShadowColor = "#000000",
			sectorTopColor = '#5780aa',
			sectorBottomColor = '#3b648d',
			sectorOpacity = 0.72,
			sectorLineColor = '#e1e1e1',
			fontTopColor = '#fefefe',
			fontBottomColor = '#b6b6b6',
			lastend = 0,
			offset = Math.PI / 2,
			arcsector = Math.PI * (2 / counter),
			fontSize = Math.floor(dimension/3.5),
			countdown_context = null,
			countdown_canvas = null,
			thisObjRef = null;
		
		//"getXY" method - to get the arc end position to draw the line
		function getXY(x, y, d, a){
			return {
				x: x + d * Math.cos(a),
				y: y + d * Math.sin(a)
			};
		};
		
		//"clearCanvas" method:
		function clearCanvas(){
			countdown_context.clearRect(0, 0, countdown_canvas.clientWidth, countdown_canvas.clientHeight);
		}
		
		function lineargradient(ctx, x0, y0, x1, y1, topColor, bottomColor){
			var grd = ctx.createLinearGradient(x0, y0, x1, y1);
			grd.addColorStop(0, topColor);
			grd.addColorStop(1, bottomColor);
			ctx.fillStyle = grd;
		}
		
		function radialgradient(ctx, x0, y0, r0, x1, y1, r1, startColor, endColor){
			var grd = ctx.createRadialGradient(x0, y0, r0, x1, y1, r1);
			grd.addColorStop(0, startColor);
			grd.addColorStop(0.35, startColor);
			grd.addColorStop(0.90, endColor);
			ctx.fillStyle = grd;
		}	
		
		//"drawCircle" method:
		function drawCircle(){
			countdown_context.beginPath();

			// Translucency
			countdown_context.globalAlpha = 0.75;

			// create linear gradient
			var x0 = y0 = ((height-(circleRadius*2))/2);
			var x1 = y1 = y0 + (circleRadius*2);		
			lineargradient(countdown_context, x0, y0, x1, y1, circleTopColor, circleBottomColor);
			
			// set the shadow
			countdown_context.shadowOffsetX = 2;
			countdown_context.shadowOffsetY = 2;
			countdown_context.shadowBlur    = 5;
			countdown_context.shadowColor   = 'rgba(0, 0, 0, 0.75)';

			// draw the circle
			countdown_context.arc(centerx, centery, circleRadius, Math.PI * 2, 0, true);
			
			countdown_context.closePath();
			
			countdown_context.fill();
		}
		
		function drawSector(){
			countdown_context.beginPath();
			
			// create gradient
			countdown_context.moveTo(centerx,centery);
			radialgradient(countdown_context, centerx,centery,0,centerx,centery,circleRadius, sectorTopColor, sectorBottomColor);
			
			countdown_context.arc(centerx, centery, circleRadius, 0 - offset, lastend + arcsector - offset, false);
			countdown_context.shadowOffsetX = 0;
			countdown_context.shadowOffsetY = 0;
			countdown_context.shadowBlur = 0;
			countdown_context.fill();
			countdown_context.closePath();
			
			if(counter != 1){
				countdown_context.beginPath();
				countdown_context.strokeStyle = sectorLineColor;
				countdown_context.moveTo(centerx, centery); // line 1
				var xy = getXY(centerx, centery, circleRadius, lastend + arcsector - offset)
				countdown_context.lineTo(xy.x, xy.y);			
				countdown_context.moveTo(centerx, centery); // line 1
				countdown_context.lineTo(centerx, (height/2) - circleRadius); // line 1
				countdown_context.lineWidth = 1;
				countdown_context.lineJoin = "miter";
				countdown_context.stroke();
				countdown_context.closePath();
			}
			
			lastend += arcsector;
			
			// Translucency
			countdown_context.globalAlpha = 1;
			countdown_context.font = "bold " + fontSize + "pt Arial";
			
			// create linear gradient
			var x0 = y0 = ((height-fontSize)/2);
			var x1 = y1 = y0 + fontSize;
			lineargradient(countdown_context, x0, y0, x1, y1, fontTopColor, fontBottomColor);
			
			countdown_context.shadowOffsetX = 2;
			countdown_context.shadowOffsetY = 2;
			countdown_context.shadowBlur = 5;
			countdown_context.textAlign = "center";
			countdown_context.fillText(counter, centerx, centery + (fontSize/2));
		}
		
		return {
			drawSectorsTimer: null,
			rand: null,
			countdown_canvas: null,
			countdown_context: null,
			initCounter: function(){
				this.exitCounter();
				this.rand = 'timer' + (new Date()).getTime();
				this.countdown_canvas = document.createElement('canvas');
				this.countdown_canvas.setAttribute('width', width);
				this.countdown_canvas.setAttribute('height', height);
				this.countdown_canvas.setAttribute('style', 'align: center');
				this.countdown_canvas.setAttribute('id', this.rand);
				this.countdown_canvas.setAttribute('style', 'position:absolute');
				this.countdown_canvas.setAttribute('style', 'left:0px');
				this.countdown_canvas.setAttribute('style', 'top:0px'); 
					
				//document.getElementById(parentObj).appendChild(this.countdown_canvas);
				parentObj.appendChild(this.countdown_canvas);
				this.countdown_context = document.getElementById(this.rand).getContext("2d");

				countdown_canvas = this.countdown_canvas;
				countdown_context = this.countdown_context;
				this.showCounter();
				var thisReference = this;
				//this.drawSectorsTimer = setInterval(function(){thisReference.drawAnimCounter()}, 1000);
			},
			showDefaultCounter:function(){
				var thisReference = this;
				this.drawSectorsTimer = setInterval(function(){thisReference.showCounter()}, countDownSeconds*1000/defaultCounter);
			},
			exitCounter: function(){
				var removeCanvas = document.getElementById(this.rand);
				try{
					clearCanvas();
					removeCanvas.parentNode.removeChild(removeCanvas);
				} catch (e){ }
				if(useDefault) {
					counter = defaultCounter;
				} else {
					counter = countDownSeconds;
				}
				lastend = 0;
				clearInterval(this.drawSectorsTimer);
			},
			drawAnimCounter: function(){
				clearCanvas();
				drawCircle();
				drawSector();
			},
			showCounter: function(){
				this.drawAnimCounter();
				counter--;
				if(counter < 0) this.exitCounter();
			}
		};
	};


	function findPos(obj){
        var curleft = 0;
        var curtop = 0;
        var pageY = 0;
        var pageX = 0;
        
        if (obj.offsetParent) {
            do{
                curleft += obj.offsetLeft;
                curtop += obj.offsetTop;
            }while (obj = obj.offsetParent);
        }
        
	    if(!isNaN(window.pageYOffset)) {
	       pageX = window.pageXOffset;
	       pageY = window.pageYOffset;
	    }
	    else {
	       pageY=document.body.scrollTop || document.documentElement.scrollTop;
	       pageX=document.body.scrollLeft || document.documentElement.scrollLeft;
	    }

		    
        return [curleft,curtop,pageX,pageY];
    };

	this.getFlashVersion = function(){
      // ie
      try {
        try {
          // avoid fp6 minor version lookup issues
          // see: http://blog.deconcept.com/2006/01/11/getvariable-setvariable-crash-internet-explorer-flash-6/
          var axo = new ActiveXObject('ShockwaveFlash.ShockwaveFlash.6');
          try { axo.AllowScriptAccess = 'always'; }
          catch(e) { return '6,0,0'; }
        } catch(e) {}
        return new ActiveXObject('ShockwaveFlash.ShockwaveFlash').GetVariable('$version').replace(/\D+/g, ',').match(/^,?(.+),?$/)[1];
      // other browsers
      } catch(e) {
        try {
          if(navigator.mimeTypes["application/x-shockwave-flash"].enabledPlugin){
            return (navigator.plugins["Shockwave Flash 2.0"] || navigator.plugins["Shockwave Flash"]).description.replace(/\D+/g, ",").match(/^,?(.+),?$/)[1];
          }
        } catch(e) {}
      }
      return '0,0,0';
    };

	function getBrowserWindowProps() {
        var browserWidth = 0, browserHeight = 0;
        //For checking non-IE browsers Mozilla, Safari, Opera, Chrome.
        if (typeof (window.innerWidth) == 'number') {
            browserWidth = window.innerWidth;
            browserHeight = window.innerHeight;
        }
        //All IE except version 4
        else if (document.documentElement && (document.documentElement.clientWidth || document.documentElement.clientHeight)) {               
            browserWidth = document.documentElement.clientWidth;
            browserHeight = document.documentElement.clientHeight;
        }
        //IE 4
        else if (document.body && (document.body.clientWidth || document.body.clientHeight)) {
            browserWidth = document.body.clientWidth;
            browserHeight = document.body.clientHeight;
        }
        
        return [browserWidth, browserHeight];
    };

    function regExpEscape(str)
    {
      var specials = new RegExp("[.*+?|()\&\$\\[\\]{}\\\\]", "g"); // .*+?|()[]{}\
      return str.replace(specials, "\\$&");
    };
    
	function animate(elementID, newLeft, newTop, newWidth, newHeight, time, callback)
    {
        var el = document.getElementById(elementID);
        if(el == null)
            return;
        
        var cLeft = parseInt(el.style.left);
        var cTop = parseInt(el.style.top);
        var cWidth = parseInt(el.style.width);
		        var cHeight = parseInt(el.style.height);
        
		
        var totalFrames = 1;
        if(time> 0)
            totalFrames = time/40;
        
        var fLeft = newLeft - cLeft;
        if(fLeft != 0)
            fLeft /= totalFrames;
        
        var fTop = newTop - cTop;
        if(fTop != 0)
            fTop /= totalFrames;
        
        var fWidth = newWidth - cWidth;
        if(fWidth != 0)
            fWidth /= totalFrames;
        
        var fHeight = newHeight - cHeight;
        if(fHeight != 0)
            fHeight /= totalFrames;
        
        doFrame(elementID, cLeft, newLeft, fLeft,
          cTop, newTop, fTop, cWidth, newWidth, fWidth,
          cHeight, newHeight, fHeight, callback);
    };

    function doFrame(eID, cLeft, nLeft, fLeft, cTop, nTop, fTop, cWidth, nWidth, fWidth, cHeight, nHeight, fHeight, callback)
    {
        //alert("eID" + eID + "\n cLeft: " + cLeft + " nLeft: " + nLeft + " fLeft: " + fLeft + "\n cTop: " + cTop + " nTop: " + nTop + " fTop: " + fTop + "\n cWidth: " + cWidth + " nWidth: " + nWidth + " fWidth: " + fWidth + "\n cHeight: " + cHeight + " nHeight: " + nHeight  + " fHeight: " + fHeight)
        var el = document.getElementById(eID);
        if(el == null)
            return;
        
        cLeft = moveSingleVal(cLeft, nLeft, fLeft);
        cTop = moveSingleVal(cTop, nTop, fTop);
        cWidth = moveSingleVal(cWidth, nWidth, fWidth);
        cHeight = moveSingleVal(cHeight, nHeight, fHeight);

        if(!isNaN(cLeft)){
            el.style.left = Math.round(cLeft) + 'px';
        }
        el.style.top = Math.round(cTop) + 'px';
        el.style.width = Math.round(cWidth) + 'px';
		        el.style.height = Math.round(cHeight) + 'px';
        
        //if(cLeft == nLeft && cTop == nTop && cHeight == nHeight && cWidth == nWidth)
        if(cTop == nTop && cHeight == nHeight && cWidth == nWidth)
        {
            if(callback != null)
                callback();
                        return;
        }
        
        setTimeout(function(){ doFrame(eID, cLeft, nLeft, fLeft,
            cTop, nTop, fTop, cWidth, nWidth, fWidth,
            cHeight, nHeight, fHeight, callback) }, 40);
    };

	function moveSingleVal(currentVal, finalVal, frameAmt)
    {
        if(frameAmt == 0 || currentVal == finalVal)
            return finalVal;
        
        currentVal += frameAmt;
        
        if((frameAmt> 0 && currentVal>= finalVal)
        || (frameAmt <0 && currentVal <= finalVal))
        {
            return finalVal;
        }
        
        return currentVal;
    };

	function contentLoad(obj, panelId){
        var intrLoader = obj.parentNode.firstChild;
        
        intrLoader.style.display = "none";
        obj.style.display = "block";
        obj.style.visibility = "visible";
    };

	this.cancelEvent = function(e){
        if (!e) var e = window.event;
        e.cancelBubble = true;
        if (e.stopPropagation) e.stopPropagation();        
    }; 

	function getFlashMovieObject(movieName)
    {
            if (window.document[movieName])
        {
          return window.document[movieName];
        }

        if (navigator.appName.indexOf("Microsoft Internet")==-1)
        {
            if (document.embeds && document.embeds[movieName])
                return document.embeds[movieName];
            else
                 return document.getElementById(movieName);
        }
        else // if (navigator.appName.indexOf("Microsoft Internet")!=-1)
        {
            return document.getElementById(movieName);
        }
        };
	 
}


