	 function isCanvasSupported(){
		var elem = document.getElementById( "testCanvas" );
		if(elem == null || elem == undefined) {
			elem = document.createElement('canvas');
			elem.setAttribute("id", "testCanvas");
		}
		return !!(elem.getContext && elem.getContext('2d'));
	 }

	function getBestFitMobileBannerURL(bannerElem, mobileBannerObj){
		var currentWinWidth = window.innerWidth;
		var leastDiff = 10000;	
		var bestFitBannerURL = '';
		var bestFitBannerDim = '';
		for(var currentObjKey in mobileBannerObj){
			var assetWidth = currentObjKey.split('x')[0];
			if(Math.abs(currentWinWidth - assetWidth) < leastDiff){
				leastDiff = Math.abs(currentWinWidth - assetWidth);
				bestFitBannerURL = mobileBannerObj[currentObjKey];
				bestFitBannerDim = currentObjKey;
			}
		}

		var bestFitBannerHeight = bestFitBannerDim.split('x')[1];
		if(bannerElem != null){	
						bannerElem.src = bestFitBannerURL;
						bannerElem.width = currentWinWidth;
			bannerElem.height = bestFitBannerHeight;
		}
	}

	function fireExternalTracker(url){
		var imgEl = document.createElement('img');
		var finalURL = "";

		if(!url.match("http://evs.jivox.com/jivox/serverAPIs/saveImpression.php")){
			var replaceCacheBuster0 = url.replace('[timestamp]', Math.random()); 
var replaceCacheBuster1 = replaceCacheBuster0.replace('[randomNumber]', Math.random()); 
var replaceCacheBuster2 = replaceCacheBuster1.replace('{cacheBuster}', Math.random()); 
var replaceCacheBuster3 = replaceCacheBuster2.replace('{RANDOMNUM}', Math.random()); 
var replaceCacheBuster4 = replaceCacheBuster3.replace('%%CACHEBUSTER%%', Math.random()); 
finalURL = replaceCacheBuster4;			//finalURL = replaceCacheBuster;
		}else{
			if(url.indexOf("?") == -1){
				finalURL = url + "?r=" + Math.random();
			}else{
				finalURL = url + "&r=" + Math.random();
			}
		}
		
		imgEl.src = finalURL;
	}       

	 function attachEventListener(target, eventType, functionRef, capture){
        if(typeof target.addEventListener != "undefined")
        {
            target.addEventListener(eventType, functionRef, capture);
        }else{
            target.attachEvent("on" + eventType, functionRef);
        }
     };
	 
	 this.showFlashHoverAnim = function(){
		try {playerObject.flashHoverAnimMovieObject.showHoverAnim();} catch(e) {}
	 }

	 this.hideFlashHoverAnim = function(){
		try {playerObject.flashHoverAnimMovieObject.hideHoverAnim();} catch(e) {}
	 }

	 this.openFlashPeelback = function(){
		if(playerObject.interactivityState == 'open'){
			return;
		}
		try { 
			clearInterval(playerObject.showPlayInterval);
			clearInterval(playerObject.peelCloseInt); 
		} catch(e) {}
		playerObject.flashPeelbackMovieObject.openPeelback();
		playerObject.peelCloseInt = setInterval( playerObject.closeFlashPeelback, playerObject.closeInterval);
	 }

	 this.closeFlashPeelback = function(){
		try { 
			clearInterval(playerObject.showPlayInterval);
			clearInterval(playerObject.peelCloseInt);
		} catch(e) {}
		playerObject.flashPeelbackMovieObject.closePeelback();
		playerObject.showPlayInterval = setInterval( playerObject.openFlashPeelback, playerObject.openInterval);
	 }

